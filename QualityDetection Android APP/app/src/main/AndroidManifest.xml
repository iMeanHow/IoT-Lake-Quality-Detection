<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fedming.bottomnavigationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="渤龙湖水质在线检测系统"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="uOugC9gxQtUfyGpzCNQ5NwuIL1Xtf0py" >
        </meta-data>
        <service android:name="com.baidu.location.f" android:enabled="true" android:process=":remote"> </service>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.BaiduMapActivity"
            android:label="BaiduMapActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.BaiduMapPlusActivity"
            android:label="BaiduMapActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity21"
            android:label=".Activity21"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity22"
            android:label=".Activity22"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity31"
            android:label=".Activity31"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity32"
            android:label=".Activity32"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity34"
            android:label=".Activity34"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity35"
            android:label=".Activity35"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.fedming.bottomnavigationdemo.Activity43"
            android:label=".Activity43"
